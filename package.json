    "start": "GATSBY_ENV=develop gatsby develop",
    "build": "npm run clone-and-clean && npm run generate-menu && GATSBY_VERSION=$npm_package_version GATSBY_ENV=prod gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
